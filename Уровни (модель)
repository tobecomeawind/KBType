class NewWindow(QtWidgets.QMainWindow):
    def __init__(self):
        super().__init__()
        self.setWindowTitle("New Window")
        self.setGeometry(100, 100, 200, 200)

        self.returnButton = QtWidgets.QPushButton("Return", self)
        self.returnButton.setGeometry(50, 50, 100, 30)
        self.returnButton.clicked.connect(self.return_to_main_window)

    def return_to_main_window(self):
        self.close()


class NewWindow(QtWidgets.QMainWindow):
    def __init__(self):
        super().__init__()
        self.setWindowTitle("New Window")
        self.setGeometry(100, 100, 200, 200)

        self.returnButton = QtWidgets.QPushButton("Return", self)
        self.returnButton.setGeometry(50, 50, 100, 30)
        self.returnButton.clicked.connect(self.return_to_main_window)

    def return_to_main_window(self):
        self.close()


class MyMainWindow(QtWidgets.QMainWindow):
    def __init__(self):
        super().__init__()
        self.ui = Ui_MainWindow()  # Создайте экземпляр класса Ui_MainWindow
        self.ui.setupUi(self)  # Примените метод setupUi к текущему QMainWindow

        self.setup_buttons()

    def setup_buttons(self):
        self.ui.pushButton.clicked.connect(self.open_new_window)
        self.ui.pushButton_2.clicked.connect(self.open_new_window)
        self.ui.pushButton_3.clicked.connect(self.open_new_window)
        self.ui.pushButton_4.clicked.connect(self.open_new_window)
        self.ui.pushButton_5.clicked.connect(self.open_new_window)
        self.ui.pushButton_6.clicked.connect(self.open_new_window)
        self.ui.pushButton_7.clicked.connect(self.open_new_window)
        self.ui.pushButton_8.clicked.connect(self.open_new_window)
        self.ui.pushButton_9.clicked.connect(self.open_new_window)
        self.ui.pushButton_10.clicked.connect(self.open_new_window)

    def open_new_window(self):
        self.new_window = NewWindow()
        self.new_window.show()


app = QtWidgets.QApplication(sys.argv)
mainWindow = MyMainWindow()
mainWindow.show()
sys.exit(app.exec_())




